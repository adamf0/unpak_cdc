docker exec -it cdc-kafka-1 bash

kafka-topics \
  --bootstrap-server localhost:9092 \
  --topic simpeg10.unpak_simpeg.payroll_m_pegawai \
  --delete

kafka-consumer-groups \
   --bootstrap-server localhost:9092 \
   --list

kafka-consumer-groups \
  --bootstrap-server localhost:9092 \
  --group payroll_m_pegawai_consumeto_simonev \
  --delete

kafka-consumer-groups \
  --bootstrap-server localhost:9092 \
  --group payroll_m_pegawai_consumeto_simonev \
  --topic simpeg11.unpak_simpeg.payroll_m_pegawai \
  --reset-offsets \
  --to-earliest \
  --execute